read ENTRE
$PREFIX/share/EasY_HaCk/.modules/.smbscan
else
echo "\033[31m[${W}"!"${R}"] please Enter target  Url'\'IP${W}'!' " "
sleep 5
$PREFIX/share/EasY_HaCk/.modules/.smbscan
fi


