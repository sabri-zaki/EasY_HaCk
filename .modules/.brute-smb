#!/bin/sh

R='\033[1;31m'
B='\033[0;34m'
C='\033[0;36m'
G='\033[1;32m'
W='\033[1;37m'
Y='\033[0;33m'
Y2='\033[1;33m'
clear
echo $R

figlet "              SMB"
sleep 1
figlet "        BRUTE"
sleep 1
echo ""
echo -n "\033[0;36m[*]Target Url/IP~#: "
read a
if [ ! -z $a ]
then
echo "[*] Looading... "
echo ""
nmap -p 445 --script smb-brute $a -d
